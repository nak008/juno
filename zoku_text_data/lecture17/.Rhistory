library(rstan)
rstan_options(auto_write=TRUE)
options(mc.cores = parallel::detectCores())
#ローカルレベルモデル
localLevelModel <- 'data{
int<lower = 0> N;
real<lower= 0> Y[N];
}
parameters{
real alpha[N];
real<lower = 0>s_Y;
real<lower = 0>s_a;
}
model {
for(i in 1:N)
Y[i] ~ normal(alpha[i], s_Y);
for(i in 2:N)
alpha[i] ~ normal(alpha[i-1], s_a);
}
'
#MCMC
stanlocal <-stan_model(model_code = localLevelModel)
